{
  "name": "octopusjjb-reforged",
  "version": "1.0.0",
  "backend": "server/dist/index.js",
  "targets": {
    "frontend": {
      "source": [
        "ui/src/index.html"
      ],
      "distDir": "server/public"
    },
    "backend": {
      "context": "node",
      "source": "server/src/index.ts",
      "distDir": "server/dist"
    }
  },
  "scripts": {
    "start": "node server/dist/index.js",
    "build": "npm run clean && parcel build --no-cache",
    "watch": "parcel watch --no-cache",
    "dev": "concurrently \"npm run clean && npm run build\" \"parcel serve --target frontend --no-cache --port 1234\" \"parcel watch --target backend --no-cache --port 8080\" \"nodemon server/dist/index.js\"",
    "clean": "gulp clean-old-bundles",
    "prod:start": "pm2-runtime server/dist/index.js"
  },
